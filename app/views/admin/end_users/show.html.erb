<div class="container">
    <div class="row">
        <div class="col">
            <h2><%= @end_user.last_name%><%= @end_user.fast_name %>さんのユーザー詳細</h2>
            <table class="table">
                <tr>
                    <th>ユーザーID</th>
                    <td><%= @end_user.id %></td>
                </tr>
                <tr>
                    <th>氏名</th>
                    <td><%= @end_user.last_name %><%= @end_user.fast_name %></td>
                </tr>
                <tr>
                    <th>ニックネーム</th>
                    <td><%= @end_user.nick_name %></td>
                </tr>
                <tr>
                    <th>電話番号</th>
                    <td><%= @end_user.telephone_number %></td>
                </tr>
                <tr>
                    <th>メールアドレス</th>
                    <td><%= @end_user.email %></td>
                </tr>
                <tr>
                    <th>ステータス</th>
                    <td><%= @end_user.is_deleted ? '退会' : '有効' %></td>
                </tr>
            </table>
            <%= link_to "編集する",edit_admin_end_user_path(@end_user),class:'btn btn-success mb-2' %>
        </div>
    </div>
</div>